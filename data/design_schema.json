{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "http://example.com/example.json",
	"type": "array",
	"title": "The Design Schema",
	"description": "The JSON schema for Building Design",
	"default": [],
	"additionalItems": true,
"items": {
        "$id": "#/items",
        "type": "object",
        "title": "The Items Schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
            {
                "target": "",
                "name": "first design",
                "autosave": "",
                "GUID": "58702861-0f70-4bbd-aebf-1df158d4043f"
            },
            {
                "autosave": "",
                "GUID": "58702861-0f70-4bbd-aebf-1df158d4043f",
                "target": "",
                "name": "SECOND design"
            },
            {
                "autosave": "",
                "GUID": "58702861-0f70-4bbd-aebf-1df158d4043f",
                "target": "",
                "name": "THIRD design"
            }
        ],
        "additionalProperties": true,
        "required": [
            "name",
            "target",
            "autosave",
            "GUID"
        ],
        "properties": {
            "name": {
                "$id": "#/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "first design"
                ]
            },
            "target": {
                "$id": "#/items/properties/target",
                "type": "string",
                "title": "The Target Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    ""
                ]
            },
            "autosave": {
                "$id": "#/items/properties/autosave",
                "type": "string",
                "title": "The Autosave Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    ""
                ]
            },
            "GUID": {
                "$id": "#/items/properties/GUID",
                "type": "string",
                "title": "The Guid Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "58702861-0f70-4bbd-aebf-1df158d4043f"
                ]
            }
        }
    }
}