                    "Engineering Work"
                ]
            },
            "course_code": {
                "type": "string",
                "title": "The Course_code Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "11000"
                ]
            },
            "level": {
                "type": "string",
                "title": "The Level Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "BSc"
                ]
            },
            "ECTS": {
                "type": "string",
                "title": "The Ects Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "10"
                ]
            },
            "schedule": {
                "type": "string",
                "title": "The Schedule Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "E3B"
                ]
            },
            "schedule2": {
                "type": "string",
                "title": "The Schedule2 Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
